{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index"
    },
    {
      "src": "/index",
      "dest": "api/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/app.py"
    }
  ]
}